<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{{ article.title }}</title>
    {{> default_head }}
</head>
<body>
{{> header }}
<div class="container">
    <div class="col-sm-10 col-sm-push-1">

        <h1>{{ article.title }} <small>by {{ article.author }}</small></h1>
        <img src="{{ article.imageUrl }}" alt="">
        <p>
            {{{ article.text }}}
        </p>
    </div>
</div>
</body>
</html>